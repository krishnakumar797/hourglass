{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es5",
      "lib": [ "es2016" ],
      "moduleResolution": "node",
      "noImplicitAny": false,
      "sourceMap": true,
      "noUnusedLocals": false,
      "noUnusedParameters": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "outDir": "./dist",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
  },
  "exclude": ["./node_modules"]
}
